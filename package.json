{
  "name": "cargo-project",
  "private": true,
  "version": "1.0.0",
  "description": "This is a node.js app using react-leaflet",
  "keywords": [
    "express",
    "view engine",
    "react"
  ],
  "main": "webpack-dev-server --config ./example/webpack.config.js",
  "scripts":{
     "test": "echo \"Error: no test specified\" && exit 1",
    "clean:lib": "rm -Rf ./lib",
    "compile:lib": "babel src --out-dir lib",
    "build:lib": "npm run clean:lib && npm run compile:lib",
    "build:dist": "webpack src/index.js dist/cargo-project.js & webpack -p src/index.js dist/cargo-project.min.js",
    "build": "npm run build:lib && npm run build:dist",
    "example": "webpack-dev-server --config ./example/webpack.config.js",
    "lint": "eslint ./src",
    "release:major": "npm version major && git push --follow-tags",
    "release:minor": "npm version minor && git push --follow-tags",
    "release:patch": "npm version patch && git push --follow-tags"
  },  
  "author": {
    "name": "Christos Charmatzis",
    "email": "xr_xarmatzis@outlook.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Charmatzis/cargo-project.git"
  },
  "license": "MIT",
  "peerDependencies": {
    "leaflet": "^0.7.7",
    "react-leaflet": "^0.11.5",
    "react": "^15.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.1.18",
    "classnames":"^2.2.5",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.16.1",
    "fs": "^0.0.2",
    "leaflet": "^0.7.7",
    "lodash":"^4.13.1",
    "react": "^15.0.2",
    "react-bootstrap": "^0.29.4",
    "react-dom": "^15.0.2",
    "react-leaflet": "^0.11.5",
    "react-leaflet-bing": "^1.0.1",
    "react-leaflet-google": "^1.0.2",
    "warning": "^3.0.0",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.12.1"
  }
}